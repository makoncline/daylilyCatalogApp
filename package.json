    "aws-sdk": "^2.508.0",
    "babel-plugin-import": "^1.12.0",
    "chalk": "^2.4.2",
    "connect-pg-simple": "^6.0.0",
    "connect-redis": "^3.4.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "graphile-migrate": "^0.0.17",
    "graphile-utils": "^4.4.4",
    "graphile-worker": "^0.1.0-alpha.0",
