{
  "name": "My_Project_Here",
  "version": "1.0.1",
  "private": true,
  "description": "Description of project here",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.graphql . && tsc --noEmit",
    "setup": "./scripts/setup",
    "server:src:build": "yarn codegen && cd backend && tsc && chmod +x dist/server/index.js",
    "server:src:watch": "cd backend && tsc --watch",
    "server:watch": ". ./scripts/env && nodemon --watch 'backend/dist/server/**/*.js' -x 'node --inspect=9678 -r source-map-support/register' backend/dist/server/index.js",
    "server:worker:watch": ". ./scripts/env && cd backend/dist/worker && node --inspect=9757 ../../../node_modules/.bin/graphile-worker --watch",
    "backend:server:run": ". ./scripts/env && node backend/dist/server/index.js",
    "backend:worker:run": ". ./scripts/env && cd backend/dist/worker && graphile-worker",
    "db:watch": ". ./scripts/env && graphile-migrate watch",
    "db:migrate": ". ./scripts/env && graphile-migrate",
    "db:migrate:dump": ". ./scripts/env && graphile-migrate && graphile-migrate reset --shadow && graphile-migrate migrate --shadow --force",
    "db:wipe_if_demo": ". ./scripts/env && if [ \"$DEMO\" = \"WIPE_DATABASE\" ]; then ./scripts/wipe-db; fi",
    "start": "./scripts/check-setup && yarn dev",
    "dev": "yarn && yarn server:src:build && concurrently --kill-others --names 'db,server:src,server:run,worker,codegen,test' 'yarn db:watch' 'yarn server:src:watch' 'sleep 8; yarn server:watch' 'sleep 5; yarn server:worker:watch' 'yarn codegen:watch' 'sleep 15; yarn test:watch'",
    "test": "./scripts/test",
    "test:watch": "./scripts/test --watch",
    "cypress:open": ". ./scripts/env && cypress open",
    "cypress:run": ". ./scripts/env && cypress run",
    "codegen": "graphql-codegen --config codegen.yml",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch",
    "licenses": "yarn --silent licenses generate-disclaimer > LICENSES.md",
    "client:src:build": "cd client/src && next build",
    "build": "yarn codegen && yarn client:src:build && yarn server:src:build",
    "clean": "rm -Rf backend/dist client/dist .next"
  },
  "author": "Benjie Gillam <code@benjiegillam.com>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "@apollo/react-hooks": "^3.0.0",
    "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
    "@graphile/pg-pubsub": "^4.4.3",
    "@graphile/pro": "^0.9.0",
    "@graphile/subscriptions-lds": "^4.4.4",
    "@graphql-codegen/add": "^1.5.0",
    "@types/chalk": "^2.2.0",
    "@types/connect-pg-simple": "^4.2.0",
    "@types/connect-redis": "^0.0.10",
    "@types/express-session": "^1.15.13",
    "@types/helmet": "^0.0.44",
    "@types/html-to-text": "^1.4.31",
    "@types/lodash": "^4.14.136",
    "@types/mjml": "^4.0.2",
    "@types/morgan": "^1.7.36",
    "@types/nodemailer": "^6.2.1",
    "@types/nprogress": "^0.2.0",
    "@types/passport": "^1.0.0",
    "@types/passport-github": "^1.1.5",
    "@types/pg": "^7.4.14",
    "@zeit/next-css": "^1.0.1",
